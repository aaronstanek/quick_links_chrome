{
    "manifest_version": 3,
    "name": "Quick Links",
    "version": "1.0.1",
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
      },
    "description": "Fast in-browser go-links implementation",
    "permissions": [
        "omnibox",
        "storage",
        "unlimitedStorage"
    ],
    "omnibox": { "keyword" : "go" },
    "background": {
        "service_worker": "background.js"
    }
}
