{
    "manifest_version": 3,
    "name": "Quick Links",
    "version": "0.2.3",
    "description": "Fast in-browser go-links implementation",
    "permissions": [
        "omnibox",
        "storage",
        "unlimitedStorage"
    ],
    "omnibox": { "keyword" : "go" },
    "background": {
        "service_worker": "background.js"
    }
}
