{
    "manifest_version": 3,
    "name": "Quick Links",
    "version": "1.1.3",
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
      },
    "description": "Fast in-browser go-links implementation",
    "permissions": [
        "tabs",
        "omnibox",
        "storage",
        "unlimitedStorage"
    ],
    "omnibox": { "keyword" : "go" },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "16": "icon16.png",
            "24": "icon24.png",
            "32": "icon32.png"
        },
        "default_title": "Quick Links",
        "default_popup": "popup/index.html"
    }
}
